<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
            android:bottom="@dimen/camera_mark_width"
            android:right="@dimen/camera_mark_width"
            android:top="@dimen/camera_mark_width_minus"
            android:left="@dimen/camera_mark_width_minus"
    >
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <stroke android:color="@color/colorWhiteOpaque"
                    android:width="@dimen/camera_mark_width"></stroke>

            <corners android:topLeftRadius="0dp"
                     android:bottomRightRadius="@dimen/camera_mark_radius"
                     android:topRightRadius="0dp"
                     android:bottomLeftRadius="0dp"/>
        </shape>
    </item>
</layer-list>
